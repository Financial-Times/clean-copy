{
  "manifest_version": 2,
  "name": "Clean Copy",
  "description": "Removes superfluous linebreaks from text",
  "version": "1.0",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "browser_action": {
   "default_icon": "icon-64.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardWrite",
    "clipboardRead"
   ]
}
